<% unless errors.blank? %>
    <% errors.each do |key, value| %>
        <div class="alert alert-warning">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <strong><%= key %> </strong><%= value.join(',') %></div>
    <% end %>
<% end %>